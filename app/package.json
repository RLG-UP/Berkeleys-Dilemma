{
  "name": "client-side",
  "version": "1.0.0",
  "description": "Client side config",
  "private": true,
  "dependencies": {
    "axios": "^1.7.8",
    "next": "^15.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.1"
  },
  "scripts": {
    "start": "next start",
    "build": "next build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "npm run --prefix ../",
    "client": "next dev --port 3001",
    "dev": "concurrently \"npm run server\" \"npm run client\""
